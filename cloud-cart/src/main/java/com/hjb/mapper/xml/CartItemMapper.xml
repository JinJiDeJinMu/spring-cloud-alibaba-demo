<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hjb.mapper.CartItemMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hjb.domain.po.CartItem">
        <id column="id" property="id" />
        <result column="goods_id" property="goodsId" />
        <result column="user_id" property="userId" />
        <result column="sku_id" property="skuId" />
        <result column="brand_id" property="brandId" />
        <result column="goods_name" property="goodsName" />
        <result column="goods_img" property="goodsImg" />
        <result column="price" property="price" />
        <result column="number" property="number" />
        <result column="sku_desc" property="skuDesc" />
        <result column="sku_img" property="skuImg" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, goods_id, user_id, sku_id, brand_id, goods_name, goods_img, price, number, sku_desc, sku_img, create_time
    </sql>

</mapper>
